{
  "extends": ["config:base"],
  "rangeStrategy": "pin",
  "baseBranches": ["master"],
  "schedule": [
    "before 5pm"
  ],
  "prHeader": "upgrades for dependencies",
  "assignees": [
    "ESWARPALACHARLA"
  ],
  "reviewers": ["ESWARPALACHARLA"],
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["pin", "digest"],
      "groupName": "Vulnerable Dependencies",
      "updateTypes": ["versioning"],
      "packagePatterns": [
        "^.*$"
      ],
      "labels": ["maven-deps"]
    }
  ]
}