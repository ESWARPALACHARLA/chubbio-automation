{
  "extends": ["config:base"],
  "rangeStrategy": "pin",
  "baseBranches": ["master"],
  "schedule": [
    "before 5pm"
  ],
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["pin", "digest"],
      "groupName": "Vulnerable Dependencies",
      "updateTypes": ["versioning"],
      "packagePatterns": [
        "^.*$"
      ]
    }
  ]
}